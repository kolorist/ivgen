l:if test="string-length($pText)"><xsl:if test="($pText=.)"><br /></xsl:if><xsl:value-of select="substring-before(concat($pText,'::'),'::')" /><br /><xsl:call-template name="split"><xsl:with-param name="pText" select="substring-after($pText, '::')" /></xsl:call-template></xsl:if></xsl:template><!-- TEMPLATE RỖNG --><xsl:template name="emptyTemp"><div /></xsl:template><!-- Tiền thuế của dòng hàng 5% hoặc 10% đầu tiên do pm đẩy ra --><xsl:variable name="vat5"><xsl:for-each select="//DSHHDVu/HHDVu[TSuat='5%'][1]"><thueGTGT><xsl:value-of select="TTKhac/TTin[TTruong='Tiền thuế']/DLieu" /></thueGTGT></xsl:for-each></xsl:variable><xsl:variable name="vat10"><xsl:for-each select="//DSHHDVu/HHDVu[TSuat='10%'][1]"><thueGTGT><xsl:value-of select="TTKhac/TTin[TTruong='Tiền thuế']/DLieu" /></thueGTGT></xsl:for-each></xsl:variable><!-- Tiền thuế = Thành tiền * %thuế dòng hàng 5% hoặc 10% đầu tiên --><xsl:variable name="divVat5"><xsl:for-each select="//DSHHDVu/HHDVu[TSuat='5%'][1]"><thueGTGTDiv><xsl:value-of select="(ThTien * 5 ) div 100" /></thueGTGTDiv></xsl:for-each></xsl:variable><xsl:variable name="divVat10"><xsl:for-each select="//DSHHDVu/HHDVu[TSuat='10%'][1]"><thueGTGTDiv><xsl:value-of select="(ThTien * 10 ) div 100" /></thueGTGTDiv></xsl:for-each></xsl:variable><!-- Hiệu Tiền thuế (do pm đẩy ra) và  tích (Thành tiền * %thuế) của dòng hàng 5% hoặc 10% đầu tiên --><xsl:variable name="subVat5" select="$divVat5 - $vat5" /><xsl:variable name="subVat10" select="$divVat10 - $vat10" /><xsl:variable name="thue10" select="//TToan/THTTLTSuat/LTSuat[normalize-space(TSuat)='10%']" /><xsl:variable name="thue8" select="//TToan/THTTLTSuat/LTSuat[normalize-space(TSuat)='8%']" /><xsl:variable name="thue5" select="//TToan/THTTLTSuat/LTSuat[normalize-space(TSuat)='5%']" /><!-- Khai báo biến để check đk hiển thị theo ngày hóa đơn --><xsl:variable name="checkDate" select="concat(substring($issuedDate, 1, 4), substring($issuedDate, 6, 2), substring($issuedDate, 9, 2))" /><xsl:variable name="date